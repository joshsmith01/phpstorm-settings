<templateSet group="PHP">
  <template name="esc" value="&lt;?php echo esc_html( '$SELECTION$' ) ?&gt;" description="surrond with WordPress esc_html function" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="_e" value="&lt;?php _e( &quot;$SELECTION$&quot;, &quot;text-domain&quot; ) ?&gt; $END$" description="Display translated text." toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>